{
  "name": "setbased/php-audit",
  "description": "A tool for creating audit tables and triggers for logging data changes in a MySQL database.",
  "keywords": [
    "MySQL",
    "Audit",
    "Triggers"
  ],
  "type": "tool",
  "license": "MIT",
  "require": {
    "php": ">=7.2.0",
    "ext-json": "*",
    "setbased/error-handler": "^1.0",
    "setbased/exception": "^2.0",
    "setbased/helper-code-store-mysql": "^2.0",
    "setbased/php-stratum-middle": "^5.0",
    "setbased/php-stratum-mysql": "^4.0",
    "setbased/typed-config": "^1.0",
    "symfony/console": "^4.0 || ^5.0"
  },
  "require-dev": {
    "ext-pcntl": "*",
    "ext-posix": "*",
    "phpunit/phpunit": "^8.0 || ^9.0",
    "phing/phing": "^2.0",
    "setbased/phing-extensions": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "SetBased\\Audit\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SetBased\\Audit\\Test\\": "test/"
    }
  },
  "bin": [
    "bin/audit"
  ],
  "config": {
    "bin-dir": "bin/"
  }
}
