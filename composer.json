{
  "name": "setbased/php-audit",
  "description": "A tool for creating audit tables and triggers for logging data changes in a MySQL database.",
  "keywords": [
    "MySQL",
    "Audit",
    "Triggers"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "setbased/error-handler": "^1.2.0",
    "setbased/exception": "^2.2.0",
    "setbased/helper-code-store-mysql": "^2.1.1",
    "setbased/php-stratum-middle": "^5.9.0",
    "setbased/php-stratum-mysql": "^6.4.0",
    "setbased/typed-config": "^1.1.0",
    "symfony/console": "^5.3.7"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "ext-pcntl": "*",
    "ext-posix": "*",
    "phpunit/phpunit": "^9.5.0",
    "phing/phing": "^3.0.0-RC3",
    "setbased/phing-extensions": "^3.0.1"
  },
  "autoload": {
    "psr-4": {
      "SetBased\\Audit\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SetBased\\Audit\\Test\\": "test/"
    }
  },
  "bin": [
    "bin/audit"
  ],
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
